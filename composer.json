{
    "name": "bartjuh4/translation",
    "description": "A Translation package for Laravel 4 with database and cache support",
    "license": "MIT",
    "authors": [
        {
            "name": "Waavi",
            "email": "info@waavi.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.8.*",
        "illuminate/translation": "5.8.*",
        "illuminate/database": "5.8.*",
        "waavi/model": "dev-laravel-5.8"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Waavi\\Translation": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:Bartjuh4/model.git"
        }
    ],
    "minimum-stability": "dev"
}